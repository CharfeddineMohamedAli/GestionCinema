@model IEnumerable<Domain.Entity.Participation>

@{
    ViewBag.Title = "historiqueEv";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row pricing">
        <div class="col-md-8">
            <h3 class="m_2">historique participation event</h3>
            <div class="classes">
                <div class="cardio_list">

                    @for (int i = 0; i < Model.Count() / 2; i++)
                    {
                        <div class="cardio_sublist">
                            <ul class="cardio">
                                <li><i class="clock"> </i><span>@Html.DisplayFor(modelItem => Model.ElementAt(i).Event.Titre)</span></li>
                            </ul>
                            <div class="social-media">
                                <ul>
                                    <li> <span class="simptip-position-bottom simptip-movable" data-tooltip="timetable"><a href="#" target="_blank"> </a></span></li>
                                    <li>@Html.DisplayFor(modelItem => Model.ElementAt(i).Event.DateDebut)</li>
                 
                                </ul>
                            </div>
                            <div class="clear"></div>
                        </div>
                    }
                    
                </div>
                <div class="cardio_list1">

                    @for (int i = Model.Count() / 2; i < Model.Count(); i++)
                    {
                        <div class="cardio_sublist">
                            <ul class="cardio">
                                <li><i class="clock"> </i><span>@Html.DisplayFor(modelItem => Model.ElementAt(i).Event.Titre)</span></li>
                            </ul>
                            <div class="social-media">
                                <ul>
                                    <li> <span class="simptip-position-bottom simptip-movable" data-tooltip="timetable"><a href="#" target="_blank"> </a></span></li>
                                    <li>@Html.DisplayFor(modelItem => Model.ElementAt(i).Event.DateDebut)</li>

                                </ul>
                            </div>
                            <div class="clear"></div>
                        </div>

                    }

                
                </div>
                <div class="clear"></div>
            </div>
        </div>
       
        <div class="clear"></div>
    </div>
</div>








